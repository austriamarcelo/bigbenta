<script type="text/javascript">
$('.search-sugest').hide();
$('.sugget-loader').hide();

$(document).on('click', '.btn-signout', function(){
$.ajax({
type:'get',
url: 'https://bigbenta.com/sign-out/',
}).done(function(response){

if(window.location.href.indexOf("dashboard") > -1 || window.location.href.indexOf("change-password") > -1 || window.location.href.indexOf("report") > -1 ) {
window.location = 'https://bigbenta.com/';
}else{
location.reload();
}
});
});

$('.dropdown-submenu a.test').on("click", function(e){
$(this).next('ul').toggle();
e.stopPropagation();
e.preventDefault();
});

$(document).on('keyup', '.searchbox_mobile', function(){
$('.searchbox').val($(this).val());
});

$(document).on('click', '.btn-newsletter', function(){
$('.newsletter').addClass('nwidth');
$('.btn-newsletter').addClass('btn-news');
$('.btn-newsletter').removeClass('btn-newsletter');

});

$(document).on('click', '.btn-news', function(){
$('.newsletter').removeClass('nwidth');
$('.btn-news').addClass('btn-newsletter');
$('.btn-newsletter').removeClass('btn-news');
});

$(document).on('click', '.btn-connect', function(){
var email = $('.newsletter-email').val();
$.ajax({
type:'post',
url: 'https://marketplaceapi.bigbenta.com/index.php/api/create-newsletter',
data: {app_key:'11111', email: email}
}).done(function(data){
var obj_item = JSON.parse(data);
if (obj_item.code == 200) {
alert('Successfully sent');
$('.newsletter').removeClass('nwidth');
$('.btn-news').addClass('btn-newsletter');
$('.btn-newsletter').removeClass('btn-news');
}else if (obj_item.code == 404) {
alert('Please select another email');
}else if (obj_item.code == 422) {
alert('Invalid email');
}
});
});


$('.btn-categories').focus(function() {
    $('div.cat-con').show();
    $(document).bind('focusin.cat-con click.cat-con',function(e) {
        if ($(e.target).closest('div.cat-con, .btn-categories').length) return;
        $(document).unbind('.cat-con');
        $('div.cat-con').fadeOut('medium');
    });
});

$('.btn-categories').click(function() {
$('div.cat-con').toggle();
});
</script>


<script type="text/javascript">
$('.flexbox').attr('class', '');
</script>
