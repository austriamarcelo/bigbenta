<script type="text/javascript">

searching('2009-fortuner', 'stores', '1', '1', '6', 'date_created', 'desc', '', '', '', '', '', '');
searching('2009-fortuner', 'classified', '1', '1', '6', 'date_created', 'desc', '', '', '', '', '', '');


function searching(search, category, is_primary, offset, limit, sort_by, order, city_id, item_id, category_id, user_id, condition_id, typ){

    $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/search',
            data: {app_key:'11111', search:search, category: category, is_primary:is_primary, limit:limit, offset:offset, sort_by: sort_by, order:order, city_id: city_id, item_id : item_id, category_id: category_id, user_id: user_id, condition_id: condition_id, typ: typ}
    }).done(function(response){
        var obj_item = JSON.parse(response);
    if (obj_item.code == 200) {
        var htm = '';
        if (obj_item.data.stores.code == 200) {
         var counter = 0;
        $.each(obj_item.data.stores.stores, function(index, row){ 

            counter++;
           if (counter == 3) {
            var div = '<div class = "col-md-12"></div>';
                counter=0;
           }else{
                var div = '';
           }

          htm+='<div class="col-sm-4 gallery-item-entry">';
          htm+='<div class = "col-md-12" style = "padding: 0px;">';
          htm+='<div class = "col-md-12" style = "padding: 0px;">';
          htm+='<a href = "https://bigbenta.com/stores/item/'+row.id+'/'+row.permalink+'"><img style = "max-height:120px" src = "'+row.image[0].image_sm+'"></a>';
          htm+='</div>';
          htm+='<div class = "col-md-12" style = "padding-top:10px; ">';
          htm+='Php '+row.price;
          htm+='</div>';
          htm+='<div class = "col-md-12" style = "padding: 0px;">';
          htm+='<a style = "font-weight:bold !important; " href = "https://bigbenta.com/stores/item/'+row.id+'/'+row.permalink+'">'+row.title+'</a>';
          htm+='</div>';
          htm+='<div class = "col-md-12">';
          htm+=row.store_name;
          htm+='</div>';
          htm+='<div class = "col-md-12">';
          //  $.each(row.location, function(index, area){ 
          // htm+=area.city_name;
          //  });
          htm+='</div>';
            // htm+='<div class = "col-md-12">';
            // htm+=row.created_date;
            // htm+='</div>';
          htm+='</div>';
           if (row.quantity == 0) {
            htm+='<button style = "position: absolute;bottom: 10px;left: 55%;margin-left: -50px; font-size:12px" class = "btn btn-default">Out of Stock</button>';
            }else{
            htm+='<button class = "btn btn-default btn-addcart" data-variantid = '+row.variant_id+' data-minimumorder = '+row.minimum_order+' data-storeid = '+row.store_id+' style = "position: absolute;bottom: 10px;left: 55%;margin-left: -50px; font-size:12px">Add to Cart <img class = "loader_'+row.variant_id+'" style = "width: 17px; display:none" src="assets/img/icon/ajax-loader.gif"></button>';
            }
          htm+='</div>';
          htm+='</div>'+div;

        });
     }else{
        $('.mostsearchstores').hide();
     }

      if (obj_item.data.classified.code == 200) {
         var counter = 0;
        $.each(obj_item.data.classified.classified, function(index, row){ 
            counter++;
           if (counter == 3) {
            var div = '<div class = "col-md-12"></div>';
                counter=0;
           }else{
                var div = '';
           }
        htm+='<div class="col-sm-4 gallery-item-entry">';
        htm+='<div class = "col-md-12" style = "padding: 0px;">';
        htm+='<div class = "col-md-12" style = "padding: 0px;">';
        htm+='<a href = "https://bigbenta.com/classified/item/'+row.id+'/'+row.permalink+'"><img style = "max-height:120px" src = "'+row.image[0].image_sm+'"></a>';
        htm+='</div>';
        htm+='<div class = "col-md-12" style = "padding-top:10px">';
        htm+='Php '+ row.price;
        htm+='</div>';
        htm+='<div class = "col-md-12" style = "padding: 0px;">';
        htm+='<a style = "font-weight:bold !important" href = "https://bigbenta.com/classified/item/'+row.id+'/'+row.permalink+'">'+row.title+'</a>';
        htm+='</div>';
        htm+='<div class = "col-md-12">';
        $.each(row.location, function(index, area){ 
        htm+=area.city_name;
         });
        htm+='</div>';
        htm+='<div class = "col-md-12">';
        htm+=row.created_date;
        htm+='</div>';
        
        htm+='</div>';

        htm+='<div class = "ratingx">';

        htm+='<fieldset id="demo1" class="rating" style="position: absolute;bottom: 10px; margin-left: 58px;">';

        if (row.rating == 5) {
            var five = 'checked';
        }else{
            var five = '';
        }

       if (row.rating == 4) {
            var four = 'checked';
        }else{
            var four = '';
        }

        if (row.rating == 3) {
            var three = 'checked';
        }else{
            var three = '';
        }

        if (row.rating == 2) {
            var two = 'checked';
        }else{
            var two = '';
        }

        if (row.rating == 1) {
            var one = 'checked';
        }else{
            var one = '';
        }

        htm+='<input class="stars" type="radio" id="star5" name="rating'+row.id+'" value="5" '+five+'/>';
        htm+='<label class = "full" for="star5" title="Awesome - 5 stars"></label>';
        htm+='<input class="stars" type="radio" id="star4" name="rating'+row.id+'" value="4" '+four+'/>';
        htm+='<label class = "full" for="star4" title="Pretty good - 4 stars"></label>';
        htm+='<input class="stars" type="radio" id="star3" name="rating'+row.id+'" value="3" '+three+'/>';
        htm+='<label class = "full" for="star3" title="Meh - 3 stars"></label>';
        htm+='<input class="stars" type="radio" id="star2" name="rating'+row.id+'" value="2" '+two+'/>';
        htm+='<label class = "full" for="star2" title="Kinda bad - 2 stars"></label>';
        htm+='<input class="stars" type="radio" id="star1" name="rating'+row.id+'" value="1" '+one+'/>';
        htm+='<label class = "full" for="star1" title="Sucks big time - 1 star"></label>';
        htm+='</fieldset>';

        htm+='</div>'; 
        htm+='</div>'+div;
       
        });
     }else{
      $('.mostsearchclassified').hide();
     }

     if (category == 'stores') {
         $('.append_list_stores').html(htm);
     }else{
         $('.append_list_classified').html(htm);
     }
   
   }
 });

}

$(document).on('click', '.btn-small-img', function(){
  var id = $(this).attr('data-id');
  var img = $(this).attr('src');
  $('.img-src-append-'+id).attr('src', img);
});


</script>








