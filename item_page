            <button class = "btn btn-success btn-sent" disabled> Send message <img class = "loader" style = "width: 30px; display:none" src="https://bigbenta.com/assets/img/icon/ajax-loader.gif"></button> <span> or </span> <a href="javascript:void(0)" class = "btn-seller"> See seller's profile</a>
          </div> 
      </div>

    </div>

</div>



 <script src='https://www.google.com/recaptcha/api.js'></script>

<script type="text/javascript">
  
  $(document).on('click', '.btn-sent', function(){



    if ($("#g-recaptcha-response").val()) {
      $('.loader').show();
        $.ajax({
            type: 'POST',
            url: 'https://bigbenta.com/assets/recaptcha/verify.php', // The file we're making the request to
            dataType: 'html',
            async: true,
            data: {
                captchaResponse: $("#g-recaptcha-response").val() // The generated response from the widget sent as a POST parameter
            },
            success: function (data) {

              if (data == 'good') {

                var email = $('.email').val();
                var contact_number = $('.contact_number').val();
                var message = $('.message').val();
                    $.ajax({
                        type:'post',
                        url: 'https://marketplaceapi.bigbenta.com/index.php/api/send-message',
                        data: {app_key:'11111', email_sender:email, contact_number:contact_number, message: message, item_id:'10452', sender_user_id: ''}
                }).done(function(response){
                      var obj_item = JSON.parse(response);
                      if (obj_item.code == 200) {
                          alert('Successfully sent');
                          location.reload();
                      }

                      if (obj_item.code == 422) {

                      if (obj_item.data.email_sender == undefined) {
                          $('.msg-email').html('');
                      }else{
                          $('.msg-email').html('Email is required');
                          $('.loader').hide();
                      }

                      if (obj_item.data.contact_number == undefined) {
                          $('.msg-contact_number').html('');
                      }else{
                          $('.msg-contact_number').html('Contact number is required');
                          $('.loader').hide();
                      }

                      if (obj_item.data.message == undefined) {
                          $('.msg-message').html('');
                      }else{
                          $('.msg-message').html('Message is required');
                          $('.loader').hide();
                      }
                      }

                     });
             
            }else{
               $('.loader').hide();
              alert('Verification invalid');
            }
 }

          });
      }
 });



    $(document).on('click', '.btn-seller', function(){
      var src = $('.btn-userprofile').attr('href');
      window.location = src;
  });

    $(document).on('click', '.btn-agreement', function(){
    if($(this).is(':checked')){
        $('.btn-sent').attr('disabled', false);
    }else{
        $('.btn-sent').attr('disabled', true);
    }
 });

</script>
                                      </div>
                                                                        

