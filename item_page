<script type="text/javascript">

$(document).on('click', '.btn-soon', function(){
  alert('Opening soon!');
});

    $(document).on('click', '.btn-search', function () {
        var search = $('.searchbox').val();
        if (search.length == 0) {
            alert('Search box should not be empty');
          }else{
            search = search.replace(/\s+/g, '-').toLowerCase();
             
             
               window.location = "https://bigbenta.com/search/" + search;
             

                          window.location = "https://bigbenta.com/search/" + search;
            
                      }

        
    });


    $('.searchbox').keyup(function (e) {
      $('.sidebar-suggest .remove-suggest').removeClass('append-suggest');
      var search = $('.searchbox').val();
      suggest(search, '', '1', 0, 12, 'date_created', 'desc', '', '', '', '', '', '', '', '');
        if (e.keyCode == 13)
        {
          if (search.length == 0) {
            alert('Search box should not be empty');
            }else{
            search = search.replace(/\s+/g, '-').toLowerCase();
             
             
               window.location = "https://bigbenta.com/search/" + search;
             

                          window.location = "https://bigbenta.com/search/" + search;
            
                       }
        }
        if (search.length !=0) {
         $('.search-sugest').show();
        }else{
         $('.search-sugest').hide();
        }
    });



   


    $(document).on('click', '.btn-addcart', function(){
    var variant_id = $(this).attr('data-variantid');
    var minimum_order = $(this).attr('data-minimumorder');
    var store_id = $(this).attr('data-storeid');
    $('.success-addedcart').show();
    $('.loader_'+variant_id).show();

    $.ajax({
            type:'get',
            url: 'https://bigbenta.com/addcart/'+variant_id+'/'+minimum_order+'/'+store_id,
            data: {}
    }).done(function(data){

        counter();
        setTimeout(function(){
          $('.loader_'+variant_id).hide();
          $('.success-addedcart').hide();
          window.location = 'https://bigbenta.com/cart';
        }, 1000);


        });
    });

counter();
function counter(){

  $.ajax({
            type:'get',
            url: 'https://bigbenta.com/addcart/counter',
            data: {}
    }).done(function(data){

       $('.cart_counter').html(data);

        });

}





  $(document).on('change', '.btn-updateqty', function(){
      var qty = $(this).val();
      var key_id = $(this).attr('data-keyid');
       $.ajax({
            type:'get',
            url: 'https://bigbenta.com/addcart/update/quantity/'+key_id+'/'+qty,
            data: {}
    }).done(function(data){

       cart_list();

        });

  });

  $(document).on('click', '.btn-cartdel', function(){
      var key_id = $(this).attr('data-keyid');

       $.ajax({
            type:'get',
            url: 'https://bigbenta.com/addcart/delete/item/cart/'+key_id,
            data: {}
    }).done(function(data){

      cart_list();
      counter();

        });

  });

     cart_list(); 


  function cart_list(){
    $('.text-loader').show();
   $.ajax({
            type:'get',
            url: 'https://bigbenta.com/cart-overview/product_detail',
            data: {}
    }).done(function(data){

      $('.text-loader').hide();
      $('.listoverview').html(data);

        });

  }

append_shipping();
  function append_shipping(){

   $.ajax({
            type:'get',
            url: 'https://bigbenta.com/addcart/shipping-information',
            data: {}
    }).done(function(data){

      $('.append_shipping').html(data);

        });

  }


  $(document).on('change', '.provinces', function(){

        var provCode = $(this).val();
        var country_id =  $(this).find(':selected').attr('data-country');

         $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/cart-city',
            data: {app_key:'11111', provCode: provCode}
    }).done(function(data){
         var obj_item = JSON.parse(data);
         var htm = '';
         $.each(obj_item.data, function(index, row){ 

            htm +='<option value = "'+row.citymunCode+'" data-price = "'+row.v_cargo_excess_price+'">'+row.citymunDesc+'</option>';

         });
         $('.city').html(htm);
    });


     $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/cart-country',
            data: {app_key:'11111', country_id: country_id}
    }).done(function(data){
         var obj_item = JSON.parse(data);
         $.each(obj_item.data, function(index, row){ 
             $('.country').val(row.name);
             $('.country').attr('data-country', row.country_id);
         });
        
    });


  });


  $(document).on('change', '.city', function(){

        var citymunCode = $(this).val();

         $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/cart-brgy',
            data: {app_key:'11111', citymunCode: citymunCode}
    }).done(function(data){
         var obj_item = JSON.parse(data);
         var htm = '';
         $.each(obj_item.data, function(index, row){ 

            htm +='<option value = "'+row.id+'" data-price = "'+row.price+'">'+row.brgyDesc+'</option>';

         });

         $('.barangay').html(htm);

    });

  });



  $(document).on('click', '.btn-shipping', function(){

    var fullname = $('.fullname').val();
    var email = $('.email').val();
    var contactnumber = $('.contactnumber').val();
    var provinces = $('.provinces').val();
    var city = $('.city').val();
    var unitnumber = $('.unitnumber').val();
    var street = $('.street').val();
    var barangay = $('.barangay').val();
    var zipcode = $('.zipcode').val();
    var country_id =  $('.country').attr('data-country');
    var togo_price =  $('.barangay').find(':selected').attr('data-price');
    var v_cargo_excess_price = $('.city').find(':selected').attr('data-price');

       $.ajax({
            type:'post',
            url: 'https://bigbenta.com/set/shipping/information',
            data: {

                fullname: fullname, 
                email: email, 
                contactnumber: contactnumber, 
                provinces: provinces, 
                city: city, 
                unitnumber: unitnumber, 
                street: street, 
                barangay: barangay, 
                zipcode: zipcode, 
                country_id: country_id,
                togo_price: togo_price,
                v_cargo_excess_price: v_cargo_excess_price

            }
    }).done(function(data){

      window.location = 'https://bigbenta.com/payment';

        });

  });


$(document).on('click', '.btn-continuepayment', function(){
     $('.loader').show();
    var payment_id = $(this).attr('data-paymentmethod');
    var payment_url = $(this).attr('data-paymenturl');
    var payment_code = $(this).attr('data-merchant');
    var data_randomcode = $(this).attr('data-randomcode');

    if (payment_id.length == 0) {
      alert('Please select payment method');
       $('.loader').hide();
    }else{

   $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/generate-code',
            data: {
                app_key:'11111'
            }
        }).done(function(code){

      var referenceno = 'BB' + payment_id + code;
      var total_price_with_percent_and_shipping = $('.total_price_with_percent_and_shipping').html();
      var sign_amount = total_price_with_percent_and_shipping.toString().replace(/\./g, "");
      var signature = iPay88Signature(data_randomcode + referenceno + sign_amount + "PHP");
      var fullname = $('.fullname').val();
      var email = $('.email').val();
      var contactnumber = $('.contactnumber').val();

      $('.action_payment').attr('action', payment_url);
      $('.merchantcode').val(payment_code);
      $('.payment_id').val(payment_id);
      $('.referenceno').val(referenceno);
      $('.amount').val(total_price_with_percent_and_shipping);
      $('.buyername').val(fullname);
      $('.buyeremail').val(email);
      $('.buyercontact').val(contactnumber);
      $('.buyersignature').val(signature);

      var shipping_information = [];
      shipping_information[0] =  $('.fullname').val();
      shipping_information[1] =  $('.email').val();
      shipping_information[2] =  $('.contactnumber').val();
      shipping_information[3] =  $('.provinces').val();
      shipping_information[4] =  $('.city').val();
      shipping_information[5] =  $('.unitnumber').val();
      shipping_information[6] =  $('.street').val();
      shipping_information[7] =  $('.barangay').val();
      shipping_information[8] =  $('.zipcode').val();
      shipping_information[9] =  $('.country').attr('data-country');
      shipping_information[10] = $('.barangay').find(':selected').attr('data-price');
      shipping_information[11] = $('.city').find(':selected').attr('data-price');

      var list_item = [];
      $('.variant_id').each(function(){
          var list = [];
          var variant_id = $(this).attr('data-variantid');
          list[0] = variant_id;
          list[1] = $('.store_item_id_'+variant_id).html();
          list[2] = $('.form_id_'+variant_id).html();
          list[3] = $('.item_name_'+variant_id).html();
          list[4] = $('.quantity_'+variant_id).html();
          list[5] = $('.price_'+variant_id).html();
          list[6] = $('.item_percentage_'+variant_id).html();
          list[7] = $('.total_price_'+variant_id).html();
          list[8] = $('.total_weight_'+variant_id).html();
          list_item.push(list);
      });

        var buyer_transaction = [];
        buyer_transaction[0] = $('.total_quantity').html();
        buyer_transaction[1] = $('.shipping_fee').html();
        buyer_transaction[2] = $('.total_price_with_percentage').html();
        buyer_transaction[3] = $('.original_price').html();
        buyer_transaction[4] = $('.original_price_with_shipping').html();
        buyer_transaction[5] = payment_id;
        buyer_transaction[6] = referenceno;
        buyer_transaction[7] = signature;
        buyer_transaction[8] = 'OR-' + code;

        var array_data_for_email = $('.array_data_for_email').html();

       $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/create-cart',
            data: {
                app_key:'11111',
                shipping_information: shipping_information, 
                list_item: list_item,
                buyer_transaction: buyer_transaction,
                array_data_for_email: array_data_for_email
            }

        }).done(function(data){
          $('.btn-continue-payment').click();
        });

         });
      
    }

});

$(document).on('click', '.btn-payment-type', function(){
    var payment_id = $(this).attr('data-payment');
    var payment_url = $(this).attr('data-payment-url');
    var merchant = $(this).attr('data-merchant');
    var randomcode = $(this).attr('data-randomcode');

    $('.btn-continuepayment').attr('data-paymentmethod', payment_id);
    $('.btn-continuepayment').attr('data-paymenturl', payment_url);
    $('.btn-continuepayment').attr('data-merchant', merchant);
    $('.btn-continuepayment').attr('data-randomcode', randomcode);
});


$(document).on('click', '.btn-next', function(){
    $('.loader').show();

});



category1('classified-category', 'classified');
category1('store-category', 'store');
category1('services-category', 'services');

function category1(url, type){
    $.ajax({
        type:'post',
        url: 'https://marketplaceapi.bigbenta.com/index.php/api/'+url,
        data: {app_key:'11111'}
}).done(function(response){
    var obj_item = JSON.parse(response);
    if (obj_item.code == 200) {
    var htm = '';
    $.each(obj_item.data, function(index, row){ 

        htm+='<div class="cat-left-drop-menu-left">';

        if (type == 'classified') {
            htm+='<a class="menu-item-heading" href="https://bigbenta.com/classified/category/'+row.category_id+'/'+row.permalink+'">'+row.categories+' </a>';
        }
        if (type == 'store') {
          if (row.count != '0') {
            htm+='<a class="menu-item-heading" href="https://bigbenta.com/stores/category/'+row.category_id+'/'+row.permalink+'">'+row.categories+'</a>';
          }
        }
        if (type == 'services') {
            htm+='<a class="menu-item-heading" href="">'+row.categories+' </a>';
        }


      htm+='<ul>';
        $.each(row.subcategory, function(index, row_sub){ 
          htm+='<li>';
            if (type == 'classified') {
                htm+='<a style = "font-weight:normal !important" class="menu-item-heading" href="https://bigbenta.com/classified/category/'+row_sub.category_id+'/'+row_sub.permalink+'">'+row_sub.name+' </a>';
            }
            if (type == 'store') {
               if (row_sub.count != '0') {
                htm+='<a style = "font-weight:normal !important" class="menu-item-heading" href="https://bigbenta.com/stores/subcategory/'+row_sub.category_id+'/'+row_sub.permalink+'">'+row_sub.name+'</a>';
                }
            }
            if (type == 'services') {
                htm+='<a style = "font-weight:normal !important" class="menu-item-heading" href="">'+row_sub.name+' </a>';
            }
          htm+='</li>';
        });
      htm+='</ul>';           


      htm+='</div>';


    });
        if (type == 'classified') {
            $('.classified-categories-web').html(htm);
        }
        if (type == 'store') {
            $('.store-categories-web').html(htm);
        }
        if (type == 'services') {
            $('.services-categories-web').html(htm);
        }
        
    }
});
}


// This is for updating profile

$('.date_of_birth').datepicker({

format: 'yyyy-mm-dd'
  

});

$(document).on('click', '.btn-update-profile', function(){

var email = $('.email').val();
var first_name = $('.first_name').val();
var last_name = $('.last_name').val();
var mobile = $('.mobile').val();
var phone = $('.phone').val();
var gender = $('.gender').val();
var about = $('.about').val();
var date_of_birth = $('.date_of_birth').val();
var unit = $('.unit').val();
var street = $('.street').val();
var brgy = $('.brgy').val();
var city = $('.city').val();
var region = $('.region').val();
var country = $('.country').attr('data-id');
var zipcode = $('.zipcode').val();

if ($('.profile_photo').val() != '') {

    var reader = new FileReader();
    reader.onload = function (e) {

        img = e.target.result;
        update_profile(img, email, first_name, last_name, mobile, phone, gender, unit, street, brgy, city, region, country, zipcode, about, date_of_birth);

    }

    reader.readAsDataURL($('.profile_photo').prop('files')[0]);

    }else{

        update_profile('', email, first_name, last_name, mobile, phone, gender, unit, street, brgy, city, region, country, zipcode, about, date_of_birth);

    }

});


function update_profile(img, email, first_name, last_name, mobile, phone, gender, unit, street, brgy, city, region, country, zipcode, about, date_of_birth){
var form_data = new FormData();
form_data.append('image', img);
form_data.append('email', email);
form_data.append('first_name', first_name);
form_data.append('last_name', last_name);
form_data.append('mobile', mobile);
form_data.append('phone', phone);
form_data.append('gender', gender);
form_data.append('about', about);
form_data.append('date_of_birth', date_of_birth);
form_data.append('unit', unit);
form_data.append('street', street);
form_data.append('area_village', brgy);
form_data.append('city_id', city);
form_data.append('region_id', region);
form_data.append('country_id', country);
form_data.append('zip_code', zipcode);
form_data.append('app_key', '11111');
form_data.append('token', '');
form_data.append('user_id', '');
$('.loader').show();
$.ajax({
    url: 'https://marketplaceapi.bigbenta.com/index.php/api/user-update',
    dataType: 'text',
    cache: false,
    contentType: false,
    processData: false,
    data: form_data,
    type: 'post',
    success: function (response) {
        var obj_item = JSON.parse(response);
        
        if (obj_item.code == 200) {
            $('.loader').hide();
             
               
                alert('Successfully Updated');
              location.reload();
                                      
        }

        if (obj_item.code == 409) {
          $('.loader').hide();
            alert(obj_item.data.email);
        }

        if (obj_item.code == 422) {
          $('.loader').hide();
            alert('Fields with an * is required');
        }

        if (obj_item.code == 404) {
          $('.loader').hide();
            alert(obj_item.data);
        }

    }
});
}

$(document).on('change', '.region', function(){
    var region_id = $(this).val();
    var country_id =  $(this).find(':selected').attr('data-country-id');
    get_city(region_id, country_id);
});


function get_city(region_id, country_id){

     $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/cities',
            data: {app_key:'11111', region_id: region_id}
    }).done(function(response){

        var obj_item = JSON.parse(response);
        var htm = '';
        if (obj_item.code == 200) {
             $.each(obj_item.data, function(index, row){ 
                htm+='<option value = "'+row.city_id+'">'+row.name+'</option>';
            });
            $('.city').html(htm);
        }

    });


     $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/country',
            data: {app_key:'11111', country_id: country_id}
    }).done(function(response){

        var obj_item = JSON.parse(response);

        if (obj_item.code == 200) {
             $.each(obj_item.data, function(index, row){ 
                 $('.country').val(row.name);
                 $('.country').attr('data-id', row.country_id);
            });
           
        }

    });

}


//-> This is for search suggestion

function suggest(search, category, is_primary, offset, limit, sort_by, order, city_id, item_id, category_id, user_id, condition_id, typ, type, is_related_results){
  $('.sugget-loader').show();
    $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/search',
            data: {app_key:'11111', search:search, category: category, is_primary:is_primary, limit:limit, offset:offset, sort_by: sort_by, order:order, city_id: city_id, item_id : item_id, category_id: category_id, user_id: user_id, condition_id: condition_id, typ: typ, type: type, is_suggest: 1}
    }).done(function(response){
        var obj_item = JSON.parse(response);

    if (obj_item.code == 200) {

        var htm = '';
    
        if (obj_item.data.stores.code == 200) {
          if (is_related_results != "" ) {
            htm+='<label>Store items</label>';
          }
          $.each(obj_item.data.stores.stores, function(index, row){ 
            if (is_related_results == "" ) {
              htm+='<div><a style = "color:#000 !Important" href = "https://bigbenta.com/search/'+row.search_name+'/'+row.category_name_permalink+'"><i class="fa fa-angle-double-right" aria-hidden="true"></i> '+search+' <span style = "font-weight:normal !important"> category in '+row.category_name+'</span></a></div>';
            }else{
              htm+='<div><i class="fa fa-angle-right" aria-hidden="true"></i> &nbsp; <a style = "" href = "https://bigbenta.com/search/'+row.search_name+'/'+row.category_name_permalink+'"><span style = "font-weight:normal !important" class = "css_cat"> '+row.category_name+'</span></a></div>';
            }
          });
           if (is_related_results != "" ) {
            htm+='<br>';
          }
        }

        if (obj_item.data.classified.code == 200) {
          if (is_related_results != "" ) {
            htm+='<label>Classified ads items</label>';
          }
          $.each(obj_item.data.classified.classified, function(index, row){ 
            if (is_related_results == "" ) {
              htm+='<div><a style = "color:#000 !Important" href = "https://bigbenta.com/search/'+row.search_name+'/'+row.category_name_permalink+'"><i class="fa fa-angle-double-right" aria-hidden="true"></i> '+search+' <span style = "font-weight:normal !important"> category in '+row.category_name+'</span></a></div>';
            }else{
              htm+='<div><i class="fa fa-angle-right" aria-hidden="true"></i> &nbsp; <a style = "" href = "https://bigbenta.com/search/'+row.search_name+'/'+row.category_name_permalink+'"><span style = "font-weight:normal !important" class = "css_cat"> '+row.category_name+'</span></a></div>';
            }
          });
         };

         var total_item = search;
         $('.total_items').html(total_item);
         $('.append-suggest').html(htm);
         $('.sugget-loader').hide();

    }else{
          $('.append-suggest').html('No search result for: '+ search);
         $('.sugget-loader').hide();
    }


    }).error(function(){
        var err_htm = "";
        err_htm+='<div class="alert alert-warning">';
        err_htm+='<strong>Warning!</strong> Could not connet to server.';
        err_htm+='</div>';
        $('.append_annoucement').html(err_htm);
        $('.sugget-loader').hide();
    });
}



category3('classified-category', 'classified');
category3('store-category', 'store');
category3('services-category', 'services');

function category3(url, type){
  $.ajax({
    type:'post',
    url: 'https://marketplaceapi.bigbenta.com/index.php/api/'+url,
    data: {app_key:'11111'}
}).done(function(response){
  var obj_item = JSON.parse(response);
  if (obj_item.code == 200) {
  var htm = '';
  $.each(obj_item.data, function(index, row){ 
    htm+='<div class="col-sm-4 static-menu">';
    htm+='<div class="menu">';
    htm+='<ul>';
    htm+='<li><a href="" class="main-menu">'+row.categories+'</a>';
    htm+='<ul>';
  $.each(row.subcategory, function(index, row_sub){ 
    htm+='<li><a href="">'+row_sub.name+'</a></li>';
  });
    htm+='</ul>';
    htm+='</li>';
    htm+='</ul>';
    htm+='</li>';
    htm+='</ul>';
    htm+='</div>';
    htm+='</div>';
  });
    if (type == 'classified') {
      $('.classified-categories').html(htm);
    }
    if (type == 'store') {
      $('.store-categories').html(htm);
    }
    if (type == 'services') {
      $('.services-categories').html(htm);
    }
    
  }
});
}



