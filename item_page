<script type="text/javascript">

$("#demo1 .stars").click(function () {
   var check = $(this).val();
   $('.ratings').val(check);
});


  $(document).on('click', '.btn-rate', function(){
    var ratings = $('.ratings').val();
    var message = $('.message-rating').val();
    $('.loader').show();
        $.ajax({
            type:'post',
            url: 'https://marketplaceapi.bigbenta.com/index.php/api/create-item-rating',
            data: {app_key:'11111', rate: ratings, message: message, item_id:'10452', user_id: '', token: ''}
    }).done(function(response){
          var obj_item = JSON.parse(response);
          if (obj_item.code == 200) {
              alert('Successfully rate');
              location.reload();
          }
          if (obj_item.code == 422) {

            if (obj_item.data.rate == undefined) {
                  $('.msg-rate').html('');
              }else{
                  $('.msg-rate').html('Rating is required');
                  $('.loader').hide();
              }

              if (obj_item.data.message == undefined) {
                  $('.msg-message').html('');
              }else{
                  $('.msg-message').html('Rating is required');
                  $('.loader').hide();
              }

          }
         });
  });



</script>

